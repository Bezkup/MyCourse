{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "ConnectionString": {
    "Default": "Data Source=Data/MyCourse.db"  
  },

  "Courses":{
    "PerPage": 10,
    "Order":{
              "By": "Title",
              "Ascending": true,
              "Allow": ["Title", "Rating", "CurrentPrice"]
    }
  }
}


